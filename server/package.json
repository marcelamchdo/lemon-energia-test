{
  "name": "lemon-energia-test",
  "version": "1.0.0",
  "description": "Desenvolvimento de API para verificar a elegibidade do cliente. / Teste para vaga de Backend Jr. da Lemon ",
  "main": "src/api/app.js",
  "scripts": {
    "start": "node src/api/app.js",
    "dev": "nodemon src/api/app.js",
    "test": "mocha src/tests/**/*.test.js --exit",
    "commit": "cz",
    "stop": "(pm2 stop all && pm2 delete all) | grep 'PM2'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelamchdo/lemon-energia-test.git"
  },
  "keywords": [
    "nodejs",
    "mocha",
    "backend",
    "lemon"
  ],
  "author": "marcelamchdo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcelamchdo/lemon-energia-test/issues"
  },
  "homepage": "https://github.com/marcelamchdo/lemon-energia-test#readme",
  "devDependencies": {
    "chai": "^4.3.7",
    "cz-conventional-changelog": "^3.3.0",
    "git-commit-msg-linter": "^4.5.0",
    "lint-staged": "^13.1.0",
    "sinon": "^15.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "mocha": "^10.2.0",
    "mongoose": "^6.9.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "pm2": "^5.2.2"
  },
  "type": "module"
}
